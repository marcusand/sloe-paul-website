{
  "extends": ["next/core-web-vitals", "prettier"],
  "rules": {
    "no-case-declarations": 0,
    "no-debugger": 1,
    "no-empty-pattern": 1,
    "no-unreachable": 1,
    "no-unused-vars": 1,
    "react-hooks/exhaustive-deps": 0,
    "react-hooks/rules-of-hooks": 1,
    "react/react-in-jsx-scope": 0,
    "react/display-name": 1,
    "react/prop-types": 0
  }
}
